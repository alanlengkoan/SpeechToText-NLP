<h2><%= halaman %></h2>

<table border="1">
    <thead>
        <tr>
            <th>No</th>
            <th>Male</th>
            <th>Female</th>
            <th>Text</th>
        </tr>
    </thead>
    <tbody>
        <% for (let i = 0; i < list.length; i++) { %>
            <tr>
                <td><%= i + 1 %></td>
                <td>
                    <audio controls>
                        <source src="uploads/voice/male/<%= list[i].male %>" type="audio/mpeg">
                    </audio>
                </td>
                <td>
                    <audio controls>
                        <source src="uploads/voice/female/<%= list[i].female %>" type="audio/mpeg">
                    </audio>
                </td>
                <td><%= list[i].text %></td>
            </tr>
        <% } %>
    </tbody>
</table>